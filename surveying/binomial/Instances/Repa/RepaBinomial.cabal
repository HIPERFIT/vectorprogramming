name:                RepaBinomial
version:             0.1.0.0
author:              Martin Dybdal
maintainer:          dybber@dybber.dk
build-type:          Simple
cabal-version:       >=1.8

executable RepaBinomial
  build-depends:       base ==4.5.*, repa ==3.2.*, binomialbenchmark, mtl
  main-is: Main.hs
  ghc-options: -Odph -rtsopts -threaded -fno-liberate-case -funfolding-use-threshold1000 -funfolding-keeness-factor1000 -optlo-O3
-- -fllvm
